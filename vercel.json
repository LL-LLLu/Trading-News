{
  "crons": [
    {
      "path": "/api/cron/scrape",
      "schedule": "0 6 * * 1-5"
    },
    {
      "path": "/api/cron/analyze",
      "schedule": "30 6 * * 1-5"
    },
    {
      "path": "/api/cron/analyze?mode=weekly",
      "schedule": "0 7 * * 1"
    },
    {
      "path": "/api/cron/analyze?mode=forecast",
      "schedule": "0 7 * * 1,3,5"
    }
  ]
}
